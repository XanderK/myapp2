extends layout

block content

  //- Header with Background Image
  header.business-header
    section.container
      .row
        .col-lg-12
          h1.display-1.text-center.text-white.mt-4 Запчасти для вашего автомобиля
          p.text-center
            button.btn.btn-primary.btn-lg(type="button") Large button
  
  section.container.my-2
    ol.breadcrumb
      li.breadcrumb-item
        a(href="#") Home
      li.breadcrumb-item
        a(href="#") Library
      li.breadcrumb-item.active Data

  section.container
    .jumbotron
      h1.display-3 A Warm Welcome!
      h1.display-3 Тепло приветствуем!
      p.lead Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ipsa, ipsam, eligendi, in quo sunt possimus non incidunt odit vero aliquid similique quaerat nam nobis illo aspernatur vitae fugiat numquam repellat.
      a.btn.btn-primary.btn-sm(href="#") Call to action!

  section.container
    h1= title
    p Welcome to #{title}
    p Добро пожаловать в #{title}

  section.container.my-2
    include includes/map.html
  
  include includes/footer.pug

